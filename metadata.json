{
  "name": "icann-ansible",
  "version": "0.2.0",
  "author": "john.bond@icann.org",
  "summary": "small modules to install ansible and custom modules",
  "license": "MPL-2.0",
  "source": "https://github.com/icann-dns/puppet-ansible",
  "project_page": "https://github.com/icann-dns/puppet-ansible",
  "issues_url": "https://github.com/icann-dns/puppet-ansible/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.12.0"
    },
    {
      "name": "puppetlabs-vcsrepo",
      "version_requirement": ">= 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "18.04",
        "20.04",
        "22.04"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "10"
      ]
    }
  ]
}
